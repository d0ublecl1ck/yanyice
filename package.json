{
  "name": "yanyice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:api": "bun --watch server/src/index.ts",
    "dev:a": "bun run prisma:generate && concurrently -n web,api -c cyan,magenta \"next dev\" \"bun --watch server/src/index.ts\"",
    "build": "bun run prisma:generate && next build",
    "start": "next start",
    "lint": "eslint",
    "test": "bun run prisma:generate && bun test",
    "prisma:generate": "bunx prisma generate --config prisma.config.ts",
    "prisma:migrate": "bunx prisma migrate dev --config prisma.config.ts",
    "prisma:deploy": "bunx prisma migrate deploy --config prisma.config.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.5",
    "@google/genai": "^1.38.0",
    "@libsql/client": "^0.17.0",
    "@prisma/adapter-libsql": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@sinclair/typebox": "^0.34.48",
    "bcryptjs": "^3.0.3",
    "china-area-data": "^5.0.1",
    "country-state-city": "^3.2.1",
    "fastify": "^5.7.2",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tyme4ts": "^1.4.1",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^7.3.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "ignoreScripts": [
    "sharp",
    "unrs-resolver"
  ],
  "trustedDependencies": [
    "@prisma/engines",
    "better-sqlite3",
    "prisma",
    "protobufjs",
    "sharp",
    "unrs-resolver"
  ]
}
